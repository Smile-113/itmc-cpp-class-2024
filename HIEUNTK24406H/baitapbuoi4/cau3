#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int n;
    cout << " nhap vao n : ";
    cin >> n;
    if ( sqrt(n) - int(sqrt(n)) == 0)
    {
        cout << n <<" la so chinh phuong ";
    }
    else
    {
        cout << n << " khong la so chinh phuong ";
    }
    system("pause");
}